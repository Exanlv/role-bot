{
  "name": "role-bot",
  "version": "1.0.0",
  "description": "Makes roles self-assignable",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node ./src/bot-roles/index.ts",
    "test": "tslint -c ./tslint.json 'src/**/*.ts' --fix && ts-node ./src/bot-testing/index.ts",
    "lint": "tslint -c ./tslint.json 'src/**/*.ts' --fix",
    "compile": "git clean -df && tsc",
    "forever": "forever start ./src/bot-roles/index.js"
  },
  "author": "Exan",
  "license": "ISC",
  "dependencies": {
    "discord.js": "^11.4.2",
    "fs": "0.0.1-security",
    "module-alias": "^2.2.0",
    "node-fetch": "^2.6.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.17.0",
    "typescript": "^3.4.5"
  },
  "devDependencies": {
    "@types/node": "^12.0.8"
  },
  "_moduleAliases": {
    "@bot-roles": "src/bot-roles",
    "@bot-testing": "src/bot-testing",
    "@classes": "src/classes",
    "@core": "src/core"
